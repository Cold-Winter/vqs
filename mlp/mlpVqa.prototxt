layer {
  name: "ConcatData"
  #bottom: "slice_q"
  #bottom: "slice_i"
  #bottom: "slice_att"
  #bottom: "slice_a"
  bottom: "data"
  bottom: "data_Tag"
  top: "concatData"
  type: "Concat"
  concat_param {
    axis: 1
  }
}
