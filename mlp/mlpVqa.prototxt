name: "mlpVqa"
layer {
  name: "mlpVqa"
  type: "Data"
  top: "data"
  top: "label"
  include {
    phase: TRAIN
  }
  data_param {
    source: "vqaTrainAttentAllLmdbMix"
    batch_size: 8192
    backend: LMDB
  }
}
layer {
  name: "mlpVqa"
  type: "Data"
  top: "data"
  top: "label"
  include {
    phase: TEST
  }
  data_param {
    source: "vqaValAttentAllLmdbMix"
    batch_size: 2048
    backend: LMDB
  }
}
layer {
  name: "mlpVqa_Tag"
  type: "Data"
  top: "data_Tag"
  include {
    phase: TRAIN
  }
  data_param {
    source: "vqa256TagTrainNormAttentAllLmdbMix"
    batch_size: 8192
    backend: LMDB
  }
}
layer {
  name: "mlpVqa_Tag"
  type: "Data"
  top: "data_Tag"
  include {
    phase: TEST
  }
  data_param {
    source: "vqa256TagValNormAttentAllLmdbMix"
    batch_size: 2048
    backend: LMDB
  }
}
